<%= form_with model: @member, local: true do |f| %>
    <div class="form-group">
      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>
    </div>
    <div class="form-group">
      <%= f.label :membership_id %>
      <%= f.text_field :membership_id, class: 'form-control' %>
    </div>
    <div class="form-group">
      <%= f.label :contact_info %>
      <%= f.text_field :contact_info, class: 'form-control' %>
    </div>
    <div class="form-group">
      <%= f.label :membership_type %>
      <%= f.text_field :membership_type, class: 'form-control' %>
    </div>
    <%= f.submit class: 'btn btn-primary' %>
  <% end %>
  